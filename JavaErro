public int[] AdapterToInt(ArrayAdapter<String> adapter)
    {
        int[] a = new int[adapter.getCount()];
        //int[] a = new int[1];

        for (int i = 0; i < adapter.getCount(); i++)
        {
            a[i] = Integer.valueOf(adapter.getItem(i));
        }

        return a;
    }

